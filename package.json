   "devDependencies": {
      "@types/chalk": "0.4.31",
      "@types/glob": "5.0.30",
      "@types/js-beautify": "0.0.30",
      "@types/lodash": "4.14.66",
      "@types/node": "8.0.0",
      "chai": "4.0.2",
      "commitizen": "2.9.6",
      "cz-conventional-changelog": "2.0.0",
      "mocha": "3.4.2",
      "semantic-release": "6.3.6",
      "ts-node": "3.0.6",
      "tslint": "5.4.3",
      "typescript": "2.3.4"
   },
   "release": {
