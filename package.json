{
   "name": "@stratio/egeo-cli",
   "version": "0.1.1",
   "description": "cli with some utilities for egeo",
   "author": "Stratio",
   "license": "Apache 2.0",
   "bin": "bin/egeo-cli",
   "main": "./dist/index.js",
